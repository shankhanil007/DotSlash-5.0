    jQuery(document).ready(function ($) {
      let alterClass = function () {
        let ww = document.body.clientWidth;
        if (ww < 415) {
          $('.logofix').addClass('logofixer');
        } else if (ww >= 416) {
          $('.logofix').removeClass('logofixer');
        };
      };
      $(window).resize(function () {
        alterClass();
      });
      alterClass();
    });



